<?xml version="1.0" encoding="UTF-8" ?>
	<!--
		ip: ftp server ip address 
		port: ftp server port path: path of file
		store in ftp server (example dir/cc) 
		status: ftp server is run 
		weight: weight
	-->
<root>
	<default alias="a">
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/default" passwd="SK@qq.com" user="photo"
			domain="default.ifaceshow.com" status="run" weight="1" />
	</default>
	 
	<from name="pic"  alias="a" maxActive="100">
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/p1" passwd="SK@qq.com" user="photo"
			domain="p1.${host}ifaceshow.com" status="run" weight="1" />
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/p2" passwd="SK@qq.com" user="photo"
			domain="p2.${host}ifaceshow.com" status="run" weight="1" />
