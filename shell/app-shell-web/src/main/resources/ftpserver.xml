			domain="p1.${host}ifaceshow.com" status="run" weight="1" />
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/p2" passwd="SK@qq.com" user="photo"
			domain="p2.${host}ifaceshow.com" status="run" weight="1" />
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/p3" passwd="SK@qq.com" user="photo"
			domain="p3.${host}ifaceshow.com" status="run" weight="1" />
		<ftp ip="ftp.vctl.lenovo.com" port="21" path="/p4" passwd="SK@qq.com" user="photo"
